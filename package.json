{
  "name": "modify-xml",
  "version": "0.0.0",
  "description": "An easy way to modify XML documents without changing the formatting of the original",
  "main": "out/index.js",
  "module": "out/es/index.js",
  "types": "out/types/index.d.ts",
  "scripts": {
    "compile": "rollup -c",
    "types": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "test": "ava",
    "lint": "eslint rollup.config.js src test --ext .js,.ts"
  },
  "keywords": [
    "atscm",
    "xml",
    "manipulation",
    "modify",
    "parse",
    "render"
  ],
  "author": "Lukas Hechenberger <lukas.hechenberger@bachmann.info>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@ls-age/eslint-config": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "ava": "^1.4.1",
    "babel-plugin-istanbul": "^5.1.3",
    "eslint": "^5.16.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-sucrase": "^2.1.0",
    "sucrase": "^3.10.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "saxen": "^8.1.0"
  },
  "ava": {
    "files": [
      "test/**/*.{js,ts}"
    ],
    "sources": [
      "src/**/*.{js,ts}"
    ],
    "require": [
      "sucrase/register"
    ],
    "extensions": [
      "js",
      "ts"
    ],
    "babel": false,
    "compileEnhancements": false
  }
}
